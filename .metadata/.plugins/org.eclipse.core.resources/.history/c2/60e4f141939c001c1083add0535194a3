#include "profile.h"
#include "stdbool.h"

void Profile_Init(Profile *profile, float vMax, float aMax){

	profile->vMax = vMax;
	profile->aMax = aMax;

	profile->accTime=0;
	profile->constTime=0;
	profile->decTime=0;

	profile->vel=0;
	profile->acc=0;

	profile->accDisp=0;
	profile->constDisp=0;
	profile->decDisp=0;

}

